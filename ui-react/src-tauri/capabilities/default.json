{
    "$schema": "../gen/schemas/desktop-schema.json",
    "identifier": "default",
    "description": "enables the default permissions",
    "windows": ["main"],
    "permissions": [
        "core:default",
        "shell:allow-spawn",
        "shell:allow-execute",
        "shell:allow-stdin-write",
        "shell:allow-kill",
        {
            "identifier": "shell:allow-open",
            "allow": [
                {
                    "url": "https://**",
                    "cmd": ""
                }
            ]
        }
    ]
}
